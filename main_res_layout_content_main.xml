<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="700dp"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="androva.com.tblist.Home"
    tools:showIn="@layout/app_bar_main">


    <TableRow
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tableRow"
        android:paddingTop="70dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/linearLayout2"
            android:paddingBottom="20dp"
            android:layout_below="@+id/linearLayout"
            android:layout_centerHorizontal="true">

            <ImageButton
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:id="@+id/imageButton4"
                android:background="@drawable/search"
                android:layout_alignTop="@+id/linearLayout"
                android:layout_toRightOf="@+id/imageButton3"
                android:layout_toEndOf="@+id/imageButton3"
                android:layout_marginLeft="30dp"
                android:layout_marginStart="30dp"/>

            <ImageButton
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:id="@+id/imageButton3"
                android:background="@drawable/christmas"
                android:layout_below="@+id/linearLayout"
                android:layout_toLeftOf="@+id/imageButton5"
                android:layout_toStartOf="@+id/imageButton5"
                android:layout_marginLeft="30dp"
                android:layout_marginStart="30dp"/>
        </LinearLayout>
    </TableRow>

    <ImageButton
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:id="@+id/imageButton5"
        android:background="@drawable/socia"

        android:layout_below="@+id/tableRow3"
        android:layout_centerHorizontal="true" />

    <TableRow
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tableRow2"
        android:paddingTop="20dp"
        android:layout_below="@+id/tableRow"
        android:layout_alignLeft="@+id/tableRow"
        android:layout_alignStart="@+id/tableRow">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/linearLayout"
            android:layout_gravity="center"
            android:layout_below="@+id/tableRow"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true">

            <ImageButton
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:id="@+id/imageButton2"
                android:background="@drawable/menulist"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginLeft="30dp"
                android:layout_marginStart="30dp" />

            <ImageButton
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:id="@+id/imageButton"
                android:background="@drawable/profile"
                android:layout_gravity="center"
                android:layout_above="@+id/imageButton5"
                android:layout_toRightOf="@+id/imageButton2"
                android:layout_toEndOf="@+id/imageButton2"
                android:layout_marginLeft="30dp"
                android:layout_marginStart="30dp"/>

        </LinearLayout>
    </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/tableRow3"
        android:paddingTop="20dp"
        android:paddingLeft="20dp"
        android:layout_below="@+id/tableRow2"
        android:layout_alignLeft="@+id/tableRow2"
        android:layout_alignStart="@+id/tableRow2"
        android:layout_alignRight="@+id/tableRow2"
        android:layout_alignEnd="@+id/tableRow2">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/linearLayout3"
            android:layout_gravity="center"
            android:layout_below="@+id/tableRow"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true">

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageButton6"
                android:background="@drawable/cart"
                android:paddingLeft="100dp"
                android:layout_above="@+id/imageButton5"
                android:layout_alignRight="@+id/tableRow3"
                android:layout_alignEnd="@+id/tableRow3" />
        </LinearLayout>
    </TableRow>


</RelativeLayout>
